{
  "name": "metasync-site/nova-has-many-searchable",
  "description": "A Laravel Nova field for searchable has-many relationships",
  "keywords": ["laravel", "nova", "field", "has-many", "searchable"],
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "laravel/nova": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "MetasyncSite\\NovaHasManySearchable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MetasyncSite\\NovaHasManySearchable\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
